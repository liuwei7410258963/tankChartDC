package com.oket.util4biz;

/**
 * 液位的高度单位：毫米、英寸
 */
enum LevelUnit{
    MM,
    INCH
}

/**
 * 体积单位
 */
enum VolumeUnit{
    /**升*/
    LTR,
    /**立方米*/
    CUBICMETER,
    /**
     * 加仑
     */
    GAL
}

/**
 * 温度单位
 */
enum TemperatureUnit{
    /**℉——华氏度 Fahrenheit,*/
    FAH,
    /**    摄氏度 centigrade     */
    CEN
}

/**
 * 标准体积计算式采用的基准温度，中国采用20摄氏度下的体积为标准体积
 */
enum StandardTemperature{
    /**20℃为标准温度*/
    T20,
    /**15℃为标准温度*/
    T15
}
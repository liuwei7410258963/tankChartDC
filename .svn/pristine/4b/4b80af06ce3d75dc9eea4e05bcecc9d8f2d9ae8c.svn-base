package com.oket.tank4station;

/**
 * 罐存接口
 */
public interface TankInventory extends TankLevel {
	/**
	 * 获取体积
	 *
	 * @return
	 */
	double getVolume();

	/**
	 * 设置体积
	 *
	 * @param volume
	 */
	void setVolume(double volume);

	/**
	 * 获取标准体积
	 *
	 * @return
	 */
	double getStandardVolume();

	/**
	 * 设置标准体积
	 *
	 * @param volume
	 */
	void setStandardVolume(double volume);

	/**
	 * 获取水体积
	 *
	 * @return
	 */
	double getWaterVolume();

	/**
	 * 设置水体积
	 *
	 * @param volume
	 */
	void setWaterVolume(double volume);

	/**
	 * 设置标准密度
	 *
	 * @return
	 */
	double getStandardDensity();

	/**
	 * 获取标准密度
	 *
	 * @param standardDensity
	 */
	void setStandardDensity(double standardDensity);

	/**
	 * 获取油品编号
	 *
	 * @return
	 */
	String getOilCode();

	/**
	 * 设置油品编号
	 *
	 * @param oilCode
	 */
	void setOilCode(String oilCode);

	/**
	 * 获取油品名称
	 *
	 * @return
	 */
	String getOilName();

	/**
	 * 设置油品名称
	 *
	 * @param oilName
	 */
	void setOilName(String oilName);

	/**
	 * 设置最大罐存
	 *
	 * @return
	 */
	double getMaxVolume();

	/**
	 * 获取最大罐存
	 *
	 * @param maxVolume
	 */
	void setMaxVolume(double maxVolume);
}

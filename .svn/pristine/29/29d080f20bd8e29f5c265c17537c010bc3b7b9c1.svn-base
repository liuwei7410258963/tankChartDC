package com.oket.tank4station.tankchart;

/**
 * 容积表异常
 * @author sunBiaolong
 * @date 2020-04-27
 */
public class TankChartValueException extends RuntimeException {

	/**
	 * 
	 */
	private static final long serialVersionUID = 3002929625212713205L;

	/**
	 * 构造器
	 */
	public TankChartValueException() {
		this("容积表值异常或者格式不对");
	}

	/**
	 * 构造器
	 * @param message
	 */
	public TankChartValueException(String message) {
		super(message);
	}

	/**
	 * 构造器
	 * @param cause
	 */
	public TankChartValueException(Throwable cause) {
		super(cause);
	}

	/**
	 * @param message
	 * @param cause
	 */
	public TankChartValueException(String message, Throwable cause) {
		super(message, cause);
	}

}

